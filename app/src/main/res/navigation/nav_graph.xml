<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/listFragment"
        android:name="no.uio.ifi.in2000.gruppe55.ListFragment"
        android:label="List"
        tools:layout="@layout/fragment_list" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="no.uio.ifi.in2000.gruppe55.BlankFragment"
        android:label="Blank1"
        tools:layout="@layout/fragment_blank" />
    <fragment
        android:id="@+id/mapFragment"
        android:name="no.uio.ifi.in2000.gruppe55.MapFragment"
        android:label="Map"
        tools:layout="@layout/fragment_map" />

    <action
        android:id="@+id/bottom_menu_one"
        app:destination="@id/homeFragment" />

    <action
        android:id="@+id/bottom_menu_two"
        app:destination="@id/listFragment" />

    <action
        android:id="@+id/bottom_menu_three"
        app:destination="@id/mapFragment" />
    <activity
        android:id="@+id/infoActivity"
        android:name="no.uio.ifi.in2000.gruppe55.InfoActivity"
        android:label="InfoActivity" >
        <argument
            android:name="default"
            app:argType="string"
            android:defaultValue="Hello World" />
    </activity>


</navigation>