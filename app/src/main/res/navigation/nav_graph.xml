<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <action
        android:id="@+id/bottom_menu_one"
        app:destination="@id/homeFragment" />

    <action
        android:id="@+id/bottom_menu_two"
        app:destination="@id/listFragment" />

    <action
        android:id="@+id/bottom_menu_three"
        app:destination="@id/mapFragment" />

    <fragment
        android:id="@+id/listFragment"
        android:name="no.uio.ifi.in2000.gruppe55.ListFragment"
        android:label="List"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/homeFragment"
        android:name="no.uio.ifi.in2000.gruppe55.BlankFragment"
        android:label="Blank1"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_homeFragment_to_dialogFragment"
            app:destination="@id/dialogFragment" />
    </fragment>

    <fragment
        android:id="@+id/mapFragment"
        android:name="no.uio.ifi.in2000.gruppe55.MapFragment"
        android:label="Map"
        tools:layout="@layout/fragment_map" />


    <infoDialog android:id="@+id/dialogFragment"/>

</navigation>